  										<%end%>
  									<% else %>
  										<button type="button" class="btn btn-warning pull-right" name="button" data-button='{"room_id":"19","room_unique_id": "<%#= rmf.first %>","rate":"<%#= rmf.fourth.pluck("Rate").first %>"}' style="background-color:#F5BA13;box-shadow: 0 3px 0 0 #a1790c;border: 0 none;color: #000;" data-toggle="modal" data-target="#loginModal"><i class="fa fa-cart-plus" aria-hidden="true"></i>
  					 					Add to cart</button>
  									<% end %>
  								</div>
  							</div>
								<hr style="margin-top: 3px; margin-bottom: 10px; margin-left: 10px; margin-right: 10px; border: 0; border-top: 1px solid #eee;">
  						</div>

							<div class="col col-xs-3" style=" background-color: none;">

							</div>
							<div class="col col-xs-6 slick_test">
								<% (rmf['Photos'].split(',')).each do |t| %>
